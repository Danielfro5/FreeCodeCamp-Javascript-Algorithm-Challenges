<script type="text/javascript">


var result = "";
function repeatStringNumTimes(str, num) {
  
	if (num < 0) {return "";} else
{
  str = str.repeat(num);
  return str;
}
  
}

repeatStringNumTimes("*4", 3);
						
						  

